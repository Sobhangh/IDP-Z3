
-- original ---------------------------------
vocabulary {
    type num constructed from { Piet, Jan}
    same(num) : num
    negated(num) : num
} 

theory {
    { !x[num]: same(x) = x. }
    { negated(Piet) = Jan.  negated(Jan) = Piet. }
    same(Piet)= Piet.
    same(same(Piet))= negated(negated(Piet)).

}
 
display {
    view = expanded.
}
