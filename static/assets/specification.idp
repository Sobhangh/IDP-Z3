vocabulary {
    Radial
    InnerDiameter : real
    OuterDiameter : real
    Length : real
    Depth : real
    type spring constructed from {b,c,d,e,f}
    cSID : spring
}

theory {
    Radial => Depth = (OuterDiameter-InnerDiameter) / 2.0 .
    ~Radial => Depth = Length.

    2.104 =< Depth =< 2.476 <=> cSID=b.
    2.768 =< Depth =< 3.391 <=> cSID=c.
    4.294 =< Depth =< 5.206 <=> cSID=d.
    5.474 =< Depth =< 6.686 <=> cSID=e.
     8.312 =< Depth =< 10.748 <=> cSID=f.
}