vocabulary V {
    // here comes the specification of the vocabulary
    p, q: () → Bool
    i: () → Int
    r: () → Real
    type color := {red, blue, green}
    c: () → color
    type range
    ra: () → range
}

theory T:V {
    // here comes the definitions and constraints
    p() => q() & c()=blue & i()=1 & r()=2.0 & ra()=3.
}
structure S:V {
    p := true.
    range := {0..5}
}

procedure main () {
    pretty_print(Problem(T, S).formula().translate())
}