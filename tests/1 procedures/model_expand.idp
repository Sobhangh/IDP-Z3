vocabulary V {
    // here comes the specification of the vocabulary
    p, q: () → Bool
    type color := {red, blue, green}
    f: (color) → Bool
}

theory T:V {
    // here comes the definitions and constraints
    p().
    f(red).
}

structure S:V {
    // here comes the interpretation of some symbols
    q := false.
}

procedure main () {
    print(model_expand(T, S, sort=True))
}