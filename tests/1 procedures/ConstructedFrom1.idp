vocabulary V {
    type byte := {t,u}
    type color := constructed from {red, blue, green, rgb(R: byte, byte)}
}

theory T:V {
    R(rgb(t,u)) = t.
    is_rgb(rgb(t,u)).
    ~is_rgb(red).
    ~is_red(blue).
    rgb(t,u) ~= red.
}

structure S:V {
}

procedure main() {
  pretty_print(Problem(T, S).formula())
  pretty_print(Problem(T,S).expand(max=1))
}