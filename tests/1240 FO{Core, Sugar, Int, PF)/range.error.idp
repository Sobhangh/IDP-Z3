vocabulary {
	type LivingBeing := { Cat, Dog, Frog, Alice, Bob, Tom }
	animal, person : LivingBeing → Bool
	married : LivingBeing → Bool (married << person)
	spouse: LivingBeing -> LivingBeing (domain: married, range: married)
}
theory {
	animal := { Cat, Dog, Frog }.
	person := { Alice, Bob, Tom }.
    married := {Tom}.
	spouse := { Tom → Alice }.
}


procedure main() {
    pretty_print(Theory(T).formula())
    pretty_print(model_expand(T, max=1))
}
