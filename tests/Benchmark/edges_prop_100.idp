vocabulary V {
    type Node := {1..100}
    edge: Node * Node -> Bool
}

structure S  {
}

theory T:V  {
    !x, y: x ~= y => (edge(x, y) | edge(y, x)).
    !x, y: x ~= y => (~edge(x, y) | ~edge(y, x)).
    !x: ?y: x ~= y & edge(x, y).
    !x, y: x = y + 10 => edge(x, y).
}

procedure main() {
    pretty_print(model_propagate(T, S))
}
