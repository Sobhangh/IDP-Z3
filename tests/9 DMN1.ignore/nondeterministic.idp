vocabulary V {
    type Security := {BE001}
    A:() → Bool
    B:() → Bool
    C:() → Bool
    D:() → Bool
    E:() → Bool
    F:() → Bool
    G:() → Bool
    H:() → Bool
    I:() → Bool
    Eligible:(Security) → Bool
}

theory T:V {
    B() & C() => Eligible(BE001).
    B() & E() & F() => Eligible(BE001).
}

procedure main() {
	print(decision_table(T, goal_string="Eligible(BE001)"))
	print(decision_table(T, goal_string="Eligible(BE001)", verify=True))
}