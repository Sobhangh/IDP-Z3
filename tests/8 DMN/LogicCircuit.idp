

vocabulary {
	K:()
	O:()
	L:()
	Q:()
	R:()
	M:()
	N:()
	P:()
}
theory {
	//AND C
	{
		O() <- K() & L().
	}

	//OR E
	{
		R() <- O().
		R() <- Q().
		R() <- O() & Q().
	}

	//XOR A
	{
		N() <- K() & ~(L()).
		N() <- ~(K()) & L().
	}

	//XOR B
	{
		P() <- ~(N()) & M().
		P() <- N() & ~(M()).
	}

	//AND D
	{
		Q() <- N() & M().
	}

}


