	{
		Advertised_Objects() = Cars <- Age() > 18.
		Advertised_Objects() = Videogames <- Age() > 12 & ~((Age() > 18)).
		Advertised_Objects() = Toys <- true & ~((Age() > 18)|(Age() > 12)).
	}

}


