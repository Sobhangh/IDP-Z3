vocabulary V {
    q: () → Bool
    p: () → Bool
    r: Int → Bool
    Temporal p
}

theory LTC T:V {
    // Should not give an error since Now is outside existential quantifier
    (q() and  (? i in Int: i < 10 and r(i) )) or Now[p()].
}

structure S:V {
}

procedure main () {
    print(Theory(T).formula())
    pretty_print(model_expand(T,S,max=5))
}
