vocabulary V {
    // here comes the specification of the vocabulary
    type test is {0..5}
    p: () â†’ Bool
    q: test -> Bool
    Temporal p
}

theory T:V {
    // here comes the definitions and constraints
    
    {
        Start[p()].
        Next[p()] <- not Now[p()].
    }
    {
        q(0).
        !i in test: q(i+1) <- not q(i). 
    }
    
}

structure S:V {
    // here comes the interpretation of some symbols
}

procedure main () {
    print(Theory(T).formula())
    pretty_print(model_expand(T,S,max=5))
}