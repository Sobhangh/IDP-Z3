
-- original ---------------------------------
vocabulary {
    p, q, r: () → Bool
    type color := {red, blue}
    a, b, c : ()→color
}

theory {
    { p() <- q() | r(). }
    { a()=red <- p(). }
    { b()=red <- a()=red. }
    { c()=red. }
}
display {
    goal_symbol:={`b}.
    view() = expanded.
}
