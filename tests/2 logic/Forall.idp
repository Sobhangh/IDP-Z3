vocabulary V {
    type Color constructed from {red, blue, green}
    p: () → Bool
    q:(Color) → Bool
    color: ()→ Color
    r:() → Bool
}
theory T:V {
    p <=> (∀c[Color]: c=red).
    q := {red, blue}
    (∀c[Color]: ~q(c) => c=color()).
}
procedure main() {
    print(str(Problem(T).formula()))
}