vocabulary V {
    type Color := {red, blue, green}
    p: () → Bool
    q:(Color) → Bool
    color: ()→ Color
    r:(Color) → Bool
}
theory T:V {
    p() <=> (∀c in Color: c=red).
    q := {red, blue}
    (∀c in Color: ~q(c) => c=color()).
    ∀c in Color: Color(c).
    ∀c in $(`Color): Color(c).
    ∀c in q: Color(c).
    ∀c in r, c2 in q: Color(c).
}
procedure main() {
    print(str(Problem(T).formula()))
}