vocabulary V {
    has_p : () -> Bool
    p : () → 𝔹 (p ⊆ has_p)
    c : () -> Int (domain: has_p)
}

theory T:V {
    has_p() => p() & c()=3.
}

procedure main() {
    Th = Theory(T)
    print("Formula: ")
    pretty_print(Th.formula())
    print("WDF: ")
    pretty_print(Th.WDFs)
    pretty_print(model_expand(T))
}

