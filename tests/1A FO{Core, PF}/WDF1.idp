vocabulary V {
    type color := {a, b}
    col : color -> Bool
    c: () -> color
    p: col -> Bool
}

theory T:V {
    p(a).
    col(a) & p(a).
    p(c()).
    col(c()) & p(c()).
    col(c()).
    col(c()) => p(c()).
    col(c()) | p(c()).
    if col(c()) then p(c()) else true.
    ~col(c()) | p(c()).
}
procedure main() {
    Th = Theory(T)
    print("Formula: ")
    pretty_print(Th.formula())
    print("WDF: ")
    pretty_print(Th.WDFs)
}