theory T:V {

    {
        !s[Security]: Eligible(s) <- Issuer_Type(s) = Sovereign &
        				AcceptedCountry(Country(s)) & 
        				HasRating(s) =< 77.
        
       !s[Security]: Eligible(s) <- Issuer_Type(s) = Supra &
        				AcceptedSupra(Issuer(s)) &
            			HasRating(s) =< 77.
        
    }
}

