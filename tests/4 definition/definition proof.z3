
-- original ---------------------------------
vocabulary {
    type color := {red, blue}
    type code := {a,b}
    codeOf: (color) → code
    c: ()→color
}

theory {
    { !cl: ! cd in code: codeOf(cl)=cd <- (cl=c() & cd=a) | (cl~=c() & cd=b). }
    codeOf(red)=b.
}

display {
    view() = expanded.
}

