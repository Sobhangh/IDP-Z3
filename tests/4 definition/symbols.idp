vocabulary {
    type color := {red, blue, green}
    p, q: () → Bool
    important: Concept[() → Bool] → Bool
    opposite: color → color
    f: Concept -> Concept
}
theory {
  ~p() | q().
  important(`p).

  { !x in Concept[() → Bool]: important(x) <- arity(x)=0 & ~(x=`q) & $(x)(). }
  arity(`important) = 1.
  !x in Concept[() → Bool]: arity(x)<2.
  // TODO !x in Concept[Concept[()->Bool] -> Bool]: arity(x)=1 &  output_domain(x)= `Bool.
  output_domain(`opposite) = `color.
  output_domain(`opposite) ~= `Date.
  input_domain(`opposite, 1) = `color.
  input_domain(`opposite, 1) ~= `Date.
  `opposite = `opposite.
  `opposite ~= `color.
  f(`f) = `f.
  !x in Concept[color->color]: $(x)(red) is enumerated.
}
structure S:V {
  opposite := {red->blue}.
}
procedure main() {
    pretty_print(Theory(T, S).formula())
}