vocabulary {
    type color := {red, blue, green}
    p, q: () → Bool
    important: (Symbol) → Bool
    opposite: color → color
}
theory {
  ~p()|q().
  { important(`p). }

  { !`x in Symbol: important(`x) <- $(`x)(). }
  arity(`important) = 1.
  output_domain(`opposite) = `color.
  output_domain(`opposite) ~= `Date.
}
display {
  relevant(`p).
  expand(`q).
  hide(`important).
  goal(`p).
  view() = normal.
}