vocabulary {
    type color := {red, blue, green}
    p, q: () → Bool
    important: (Concept) → Bool
    opposite: color → color
    f: Concept -> Concept
}
theory {
  ~p() | q().
  important(`p).

  { !x in Concept: important(x) <- arity(x)=0 & ~(x=`q) & $(x)(). }
  arity(`important) = 1.
  !x in Concept: arity(x)<2.
  !x in Concept[Concept -> Bool]: arity(x)=1 & input_domain(x,1)=`Concept & output_domain(x)= `Bool.
  output_domain(`opposite) = `color.
  output_domain(`opposite) ~= `Date.
  input_domain(`opposite, 1) = `color.
  input_domain(`opposite, 1) ~= `Date.
  `opposite = `opposite.
  `opposite ~= `color.
  f(`f) = `f.
}
procedure main() {
    pretty_print(Theory(T).formula())
}