
-- original ---------------------------------
vocabulary {
    p, q, r: () → Bool
    type color constructed from {red, blue}
    a, b, c : ()→color
}

theory {
    { p() <- q() | r(). }
    { a()=red <- p(). }
    { b()=red <- a()=red. }
    { c()=red. }
}
display {
    goal(`b).
    view() = expanded.
}
