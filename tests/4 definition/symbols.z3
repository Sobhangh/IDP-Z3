
-- original ---------------------------------
vocabulary {
    type color := {red, blue, green}
    p, q: () → Bool
    important: (Symbol) → Bool
    opposite: color → color
    f: Symbol -> Symbol
}
theory {
  ~p() | q().
  important(`p).

  { !x in Symbol: important(x) <- arity(x)=0 & ~(x=`q) & $(x)(). }
  arity(`important) = 1.
  !x in Symbol: arity(x)<2.
  output_domain(`opposite) = `color.
  output_domain(`opposite) ~= `Date.
  input_domain(`opposite, 1) = `color.
  input_domain(`opposite, 1) ~= `Date.
  `opposite = `opposite.
  `opposite ~= `color.
  f(`f) = `f.
}
display {
  relevant(`p).
  expand(`q).
  hide(`important).
  goal(`p).
  view() = normal.
}

